{
  "name": "whisper-transcriber-app",
  "private": true,
  "version": "2.0.0",
  "type": "commonjs",
  "scripts": {
    "build": "cd client && npm install && chmod +x ./node_modules/.bin/* && npx vite build",
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "dev:full": "concurrently &quot;npm run dev&quot; &quot;npm run client&quot;",
    "test:services": "node test-services.js",
    "server:install": "cd server && npm install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@supabase/supabase-js": "^2.75.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "form-data": "^4.0.4",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.104.0",
    "parse-multipart-data": "^1.5.0",
    "pg": "^8.16.3",
    "stripe": "^19.1.0",
    "uuid": "^13.0.0",
    "youtube-transcript": "^1.2.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
